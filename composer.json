{
    "name": "plutu/plutu-laravel",
    "description": "Plutu for Laravel",
    "keywords": ["plutu", "laravel", "php", "api", "payment", "services"],
    "license": "MIT",
    "authors": [
        {
            "name": "Plutu Team",
            "email": "developers@plutu.ly",
            "homepage": "https://plutu.ly"
        },
        {
            "name": "Mohamed Endisha",
            "email": "mohamed.endisha@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "plutu/plutu-php": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "PlutuLaravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PlutuLaravel\\Providers\\PlutuServiceProvider"
            ],
            "aliases": {
                "PlutuSadad": "PlutuLaravel\\Facades\\PlutuSadad",
                "PlutuAdfali": "PlutuLaravel\\Facades\\PlutuAdfali",
                "PlutuLocalBankCards": "PlutuLaravel\\Facades\\PlutuLocalBankCards",
                "PlutuTlync": "PlutuLaravel\\Facades\\PlutuTlync",
                "PlutuMpgs": "PlutuLaravel\\Facades\\PlutuMpgs"
            }
        }
    },
    "minimum-stability": "stable"
}
